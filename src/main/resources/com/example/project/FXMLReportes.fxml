<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/24.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.project.FXMLReportes">

    <children>
        <TableView fx:id="tabla_clientes" layoutX="50.0" layoutY="50.0"
                   prefHeight="300.0" prefWidth="500.0">

            <columns>
                <TableColumn fx:id="col_cedula" prefWidth="80.0" text="Cédula" />
                <TableColumn fx:id="col_apellidos" prefWidth="100.0" text="Apellidos" />
                <TableColumn fx:id="col_nombres" prefWidth="100.0" text="Nombres" />
                <TableColumn fx:id="col_direccion" prefWidth="120.0" text="Dirección" />
                <TableColumn fx:id="col_telefono" prefWidth="80.0" text="Teléfono" />
                <TableColumn fx:id="col_correo" prefWidth="120.0" text="Correo" />
            </columns>

        </TableView>
    </children>
</AnchorPane>
